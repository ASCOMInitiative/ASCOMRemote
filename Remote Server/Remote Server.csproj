    <Compile Include="..\Shared Resources\Response Classes\AlpacaDiscoveryResponse.cs" Link="Response Classes\AlpacaDiscoveryResponse.cs" />
    <Compile Include="..\shared resources\response classes\AxisRatesResponse.cs" Link="Response Classes\AxisRatesResponse.cs" />
    <Compile Include="..\Shared Resources\Response Classes\Base64ArrayHandOffResponse.cs" Link="Response Classes\Base64ArrayHandOffResponse.cs" />
    <Compile Include="..\Shared Resources\Response Classes\Base64ArrayJsonResponse.cs" Link="Response Classes\Base64ArrayJsonResponse.cs" />
    <Compile Include="..\shared resources\response classes\BoolResponse.cs" Link="Response Classes\BoolResponse.cs" />
