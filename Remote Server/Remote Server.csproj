    <Compile Include="..\shared resources\response classes\IntArray1DResponse.cs" Link="Response Classes\IntArray1DResponse.cs" />
    <Compile Include="..\shared resources\response classes\IntArray2DResponse.cs" Link="Response Classes\IntArray2DResponse.cs" />
    <Compile Include="..\shared resources\response classes\IntArray3DResponse.cs" Link="Response Classes\IntArray3DResponse.cs" />
    <Compile Include="..\shared resources\response classes\IntResponse.cs" Link="Response Classes\IntResponse.cs" />
